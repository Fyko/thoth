    "dependencies": {
        "common-tags": "^1.8.0",
        "discord-akairo": "^8.0.0-beta.8",
        "discord.js": "github:discordjs/discord.js",
        "moment": "^2.24.0",
        "moment-duration-format": "^2.3.2",
        "mongoose": "^5.6.12",
        "node-superfetch": "^0.1.9",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@sentry/node": "^5.11.1",
        "@types/common-tags": "^1.8.0",
        "@types/dotenv": "^6.1.1",
        "@types/moment": "^2.13.0",
        "@types/moment-duration-format": "^2.2.2",
        "@types/node": "^12.7.4",
        "@types/node-fetch": "^2.5.0",
        "@types/ws": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^1.9.0",
        "@typescript-eslint/parser": "^1.9.0",
        "dotenv": "^8.0.0",
        "eslint": "^5.16.0",
        "eslint-config-marine": "^3.0.2",
        "rimraf": "^3.0.0",
        "ts-node": "^8.3.0",
        "typescript": "^3.6.2"
    },
    "scripts": {
