{
	"name": "thoth",
	"private": true,
	"version": "3.0.0",
	"description": "",
	"main": "dist/index.js",
	"imports": {
		"#util/*": "./dist/util/*.js",
		"#mw/*": "./dist/util/mw/*.js",
		"#mw": "./dist/util/mw/index.js",
		"#util/types": "./dist/util/types/index.js",
		"#constants": "./dist/util/constants.js",
		"#logger": "./dist/util/logger.js",
		"#structures": "./dist/structures/index.js"
	},
	"scripts": {
		"preinstall": "npx only-allow yarn",
		"postinstall": "husky install",
		"build": "rimraf dist/* && tsc",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --fix --ext .ts --debug",
		"pre-commit": "yarn run lint && yarn run build",
		"prestart": "yarn run build",
		"start": "node .",
		"dev": "ts-node-dev src/index.ts"
	},
	"author": "Carter Himmel <me@fyko.net>",
	"license": "UNLICENSED",
	"dependencies": {
		"@discordjs/builders": "^0.4.0",
		"@discordjs/collection": "^0.2.1",
		"@discordjs/rest": "^0.1.0-canary.0",
		"@naval-base/ms": "^3.1.0",
		"@sapphire/utilities": "^2.0.1",
		"common-tags": "^1.8.0",
		"discord-api-types": "^0.22.0",
		"discord.js": "13",
		"dotenv-cra": "^3.0.1",
		"fs-nextra": "^0.5.1",
		"pino": "^7.0.0-rc.2",
		"pretty-ms": "^7.0.1",
		"reflect-metadata": "^0.1.13",
		"rss-parser": "^3.12.0",
		"tsyringe": "^4.6.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^12.0.0",
		"@commitlint/config-angular": "^12.0.0",
		"@types/common-tags": "^1.8.0",
		"@types/eslint": "^7.2.6",
		"@types/moment-duration-format": "^2.2.2",
		"@types/ms": "^0.7.31",
		"@types/node": "^14.14.27",
		"@types/node-fetch": "^2.5.12",
		"@types/pino": "^6.3.11",
		"@typescript-eslint/eslint-plugin": "^4.15.0",
		"@typescript-eslint/parser": "^4.15.0",
		"eslint": "^7.20.0",
		"eslint-config-marine": "^8.1.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.3.1",
		"husky": "^5.0.9",
		"lint-staged": "^10.5.4",
		"mw-collegiate": "^1.0.1",
		"pino-pretty": "^3.0.0-rc.2",
		"prettier": "^2.2.1",
		"rimraf": "^3.0.2",
		"ts-node-dev": "^1.1.8",
		"tslib": "^2.1.0",
		"typescript": "^4.1.5"
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts",
		"*.{json,yml,yaml}": "prettier --write"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-angular"
		],
		"rules": {
			"type-enum": [
				2,
				"always",
				[
					"chore",
					"build",
					"ci",
					"docs",
					"feat",
					"fix",
					"perf",
					"refactor",
					"revert",
					"style",
					"test",
					"types",
					"wip"
				]
			]
		}
	},
	"volta": {
		"node": "16.6.1",
		"yarn": "1.22.11"
	}
}
