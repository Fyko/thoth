{
	"name": "thoth",
	"private": true,
	"version": "3.0.0",
	"description": "",
	"main": "dist/index.js",
	"scripts": {
		"postinstall": "husky install",
		"clean": "rimraf dist/*",
		"watch": "tsc --watch",
		"build": "tsc",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --fix --ext .ts --debug",
		"pre-commit": "yarn run lint && yarn run build",
		"prestart": "yarn run clean && yarn run build",
		"start": "node -r ts-node/register -r tsconfig-paths/register .",
		"dev": "ts-node-dev src/index.ts --watch src"
	},
	"author": "Carter Himmel <me@fyko.net>",
	"license": "UNLICENSED",
	"dependencies": {
		"@aws-sdk/client-cloudwatch": "^3.188.0",
		"@discordjs/builders": "^1.3.0",
		"@discordjs/collection": "^1.2.0",
		"@discordjs/rest": "^1.3.0",
		"@naval-base/ms": "^3.1.0",
		"@sapphire/utilities": "^3.11.0",
		"common-tags": "^1.8.2",
		"discord-api-types": "^0.37.12",
		"discord.js": "14.6.0",
		"dotenv-cra": "^3.0.2",
		"fs-nextra": "^0.5.1",
		"node-fetch": "^2",
		"pino": "^7.11.0",
		"pretty-ms": "^7.0.0",
		"reflect-metadata": "^0.1.13",
		"rss-parser": "^3.12.0",
		"tsyringe": "^4.7.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-angular": "^17.1.0",
		"@types/common-tags": "^1.8.1",
		"@types/eslint": "^8.4.6",
		"@types/ms": "^0.7.31",
		"@types/node": "^16",
		"@types/node-fetch": "^2.6.2",
		"eslint": "^8.25.0",
		"eslint-config-neon": "^0.1.38",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"mw-collegiate": "^1.0.1",
		"node-dev": "^7.4.3",
		"nodemon": "^2.0.20",
		"pino-pretty": "^7.6.1",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.1.0",
		"tslib": "^2.4.0",
		"typescript": "^4.8.4"
	},
	"lint-staged": {
		"*": "prettier --write",
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-angular"
		],
		"rules": {
			"type-enum": [
				2,
				"always",
				[
					"chore",
					"build",
					"ci",
					"docs",
					"feat",
					"fix",
					"perf",
					"refactor",
					"revert",
					"style",
					"test",
					"types",
					"wip"
				]
			]
		}
	},
	"volta": {
		"node": "16.18.0",
		"yarn": "1.22.11"
	},
	"packageManager": "yarn@3.2.4"
}
