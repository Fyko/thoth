{
	"name": "@thoth/bot",
	"private": true,
	"version": "3.0.1",
	"description": "the bot for all things ELA",
	"main": "dist/index.js",
	"type": "module",
	"imports": {
		"#util/*": "./dist/util/*",
		"#mw/*": "./dist/util/mw/*",
		"#interactions/*": "./dist/interactions/*",
		"#mw": "./dist/util/mw/index.js",
		"#util/types": "./dist/util/types/index.js",
		"#constants": "./dist/util/constants.js",
		"#logger": "./dist/util/logger.js",
		"#structures": "./dist/structures/index.js"
	},
	"scripts": {
		"generate": "node ./dist/scripts/generate.js",
		"watch": "tsc --watch",
		"build": "tsc && yarn run copy-locales",
		"start": "node .",
		"dev": "nodemon --exec node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
		"copy-locales": "cpy ./locales/**/*.* dist/ --parents"
	},
	"author": "Carter Himmel <me@fyko.net>",
	"license": "MIT",
	"dependencies": {
		"@discordjs/builders": "^1.3.0",
		"@discordjs/collection": "^1.2.0",
		"@discordjs/rest": "^1.3.0",
		"@sapphire/bitfield": "^1.0.0",
		"@sapphire/utilities": "^3.11.0",
		"@skyra/i18next-backend": "^1.1.0",
		"bullmq": "^3.1.3",
		"common-tags": "^1.8.2",
		"discord-api-types": "^0.37.14",
		"discord-verify": "^1.0.0",
		"dotenv": "^16.0.3",
		"fastify": "^4.10.2",
		"fastify-metrics": "^10.0.0",
		"fs-nextra": "^0.5.1",
		"i18next": "^21.10.0",
		"ioredis": "^5.2.3",
		"pino": "^7.11.0",
		"postgres": "^3.3.1",
		"prom-client": "^14.1.0",
		"reflect-metadata": "^0.1.13",
		"rss-parser": "^3.12.0",
		"tsyringe": "^4.7.0",
		"undici": "^5.11.0"
	},
	"devDependencies": {
		"@types/common-tags": "^1.8.1",
		"cpy-cli": "^4.2.0",
		"mw-collegiate": "^1.0.1",
		"nodemon": "^2.0.20",
		"pino-pretty": "^7.6.1",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4"
	},
	"volta": {
		"node": "18.12.1",
		"yarn": "3.2.4"
	},
	"packageManager": "yarn@3.2.4"
}
